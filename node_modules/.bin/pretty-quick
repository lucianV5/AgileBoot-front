#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dengmengnan/Desktop/AgileBoot-Front-End/node_modules/.pnpm/pretty-quick@4.2.2_prettier@3.5.3/node_modules/pretty-quick/lib/node_modules:/Users/dengmengnan/Desktop/AgileBoot-Front-End/node_modules/.pnpm/pretty-quick@4.2.2_prettier@3.5.3/node_modules/pretty-quick/node_modules:/Users/dengmengnan/Desktop/AgileBoot-Front-End/node_modules/.pnpm/pretty-quick@4.2.2_prettier@3.5.3/node_modules:/Users/dengmengnan/Desktop/AgileBoot-Front-End/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dengmengnan/Desktop/AgileBoot-Front-End/node_modules/.pnpm/pretty-quick@4.2.2_prettier@3.5.3/node_modules/pretty-quick/lib/node_modules:/Users/dengmengnan/Desktop/AgileBoot-Front-End/node_modules/.pnpm/pretty-quick@4.2.2_prettier@3.5.3/node_modules/pretty-quick/node_modules:/Users/dengmengnan/Desktop/AgileBoot-Front-End/node_modules/.pnpm/pretty-quick@4.2.2_prettier@3.5.3/node_modules:/Users/dengmengnan/Desktop/AgileBoot-Front-End/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pretty-quick/lib/cli.mjs" "$@"
else
  exec node  "$basedir/../pretty-quick/lib/cli.mjs" "$@"
fi
