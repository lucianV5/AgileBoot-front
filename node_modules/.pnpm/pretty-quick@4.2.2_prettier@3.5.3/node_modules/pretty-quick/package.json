{
  "name": "pretty-quick",
  "version": "4.2.2",
  "type": "commonjs",
  "description": "Get Pretty Quick",
  "repository": "prettier/pretty-quick",
  "author": "Lucas Azzola <@azz>",
  "maintainers": [
    "JounQin <admin@1stg.me> (https://www.1stG.me)"
  ],
  "funding": "https://opencollective.com/pretty-quick",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "bin": "lib/cli.mjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "default": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      }
    },
    "./*": "./lib/*.js",
    "./package.json": "./package.json"
  },
  "files": [
    "bin",
    "img",
    "lib",
    "!**/*.tsbuildinfo"
  ],
  "keywords": [
    "git",
    "mercurial",
    "hg",
    "prettier",
    "pretty-quick",
    "formatting",
    "code",
    "vcs",
    "precommit"
  ],
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "dependencies": {
    "@pkgr/core": "^0.2.7",
    "ignore": "^7.0.5",
    "mri": "^1.2.0",
    "picocolors": "^1.1.1",
    "picomatch": "^4.0.2",
    "tinyexec": "^0.3.2",
    "tslib": "^2.8.1"
  }
}